<%= form_with model: @team do |form| %>
<div>
  <%= form.label :name %><br>
  <%= form.text_field :name %>
  <% @team.errors.full_messages_for(:name).each do |message| %>
  <div><%= message %></div>
  <% end %>
</div>

<div>
  <%= form.label :manager %><br>
  <%= form.text_field :manager %>
  <% @team.errors.full_messages_for(:manager).each do |message| %>
  <div><%= message %></div>
  <% end %>
</div>

<div>
  <%= form.label :home %><br>
  <%= form.text_field :home %>
  <% @team.errors.full_messages_for(:home).each do |message| %>
  <div><%= message %></div>
  <% end %>
</div>

<div>
  <%= form.submit %>
</div>
<% end %>
